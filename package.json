        "grunt-template": "^0.2.3",
        "grunt-zip": "^0.16.0",
        "jsdoc": "git+https://github.com/jsdoc3/jsdoc.git",
        "fedoc-template": "git+http://14bfd38a036366f7ca90355d202038126127bd01:x-oauth-basic@github.nhnent.com/FE/Share-FEDocTemplate.git",
        "requirejs": "^2.1.14",
        "karma": "^0.12.22",
        "karma-requirejs": "^0.2.2",
        "karma-jasmine": "~0.2.0",
        "jasmine-jquery": "^2.0.5",
        "karma-coverage": "^0.2.6",
        "karma-junit-reporter": "^0.2.2",
        "karma-webdriver-launcher": "git+https://github.com/everedifice/karma-webdriver-launcher.git",
        "readme-builder": "git+https://github.com/jein-yi/readme-builder"
    },
